<%= simple_form_for [:admin, @startup] do |f| %>
  <%= f.input :name, label: "公司名称" %>
  <%= f.input :email, label: "E-mail" %>
  <%= f.input :description, label: "公司描述" %>
  <%= f.input :is_hidden, label: "隐藏" %>
  <%= f.submit "保存", data: { disable_with: "保存中..." }, class: "btn btn-primary" %>
<% end %>
